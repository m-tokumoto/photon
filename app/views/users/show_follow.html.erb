<div class="container">
	<div class="row">
		<% @user.followings.each do |user| %>
			<div class="col-md-2">
				<%= link_to user_path(user) do %>
				<div class="card" style="height: 200px;">
					<%= attachment_image_tag user, :image,:fill, fallback: "no_image-1.png", class: "card-imag-top" %>
					<div class="card-text">
						<span><%= user.name %></span>
						<i class="fas fa-user"></i>
						<span><%= user.followers.count %></span>
					</div>
				</div>
				<% end %>
			</div>
		<% end %>
	</div>
</div>